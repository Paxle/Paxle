<?xml version="1.0"?>
<!DOCTYPE mapping PUBLIC "-//EXOLAB/Castor Mapping DTD Version 1.0//EN" "http://castor.org/mapping.dtd">
<mapping>
	<include href="mapping_crawlerdoc.xml"/>

	<!--  the main command -->
	<class name="org.paxle.core.queue.Command">
		<map-to xml="Command"/>
	
		<field name="Location" type="java.lang.String">
			<description>the URL of the web resource</description>
			<bind-xml name="Location" node="element"/>
		</field>	
		
		<field name="CrawlerDocument" type="org.paxle.crawler.CrawlerDocument">
			<bind-xml name="CrawlerDocument" />
		</field>
		
		<field name="ParserDocument" type="org.paxle.parser.ParserDocument">
			<bind-xml name="ParserDocument" />
		</field>		
	</class>
	
	<!-- the parser document -->
</mapping>